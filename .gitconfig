[user]
	name = "Veezus Kreist"
	email = "almighty@veez.us"
[alias]
	ap = add -p
	b = branch
	c = commit
	co = checkout
	cp = cherry-pick
	d = diff
	dc = diff --cached
	lg = log --graph --oneline --decorate --color --all --tags
	me = merge
	mf = merge --squash
	no = show --name-only
	pr = pull --rebase
	put = push origin HEAD
	ra = rebase --abort
	rc = rebase --continue
	reauthor = commit --amend --reset-author -CHEAD
	s = status
	st = status
	squash = merge --squash
	track = --set-upstream-to
[color]
	ui = true
[core]
	excludesfile = ~/.gitignore
